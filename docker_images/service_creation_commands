docker service create --name rabbit_mq -p 5672:5672 -p 15672:15672 --network rabbit_mq_network localhost:5000/rabbit_mq

docker service create --name maria_db -p 3306:3306 --network maria_db_network -e MYSQL_ROOT_PASSWORD=root -e MYSQL_USER=petru-admin -e MYSQL_PASSWORD=petru-admin localhost:5000/maria_db

