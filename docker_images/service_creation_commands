docker service create --name spectrogram_filter --network rabbit_mq_network --network spectrogram_filter_network --network spectrogram_queue_network --network slice_genre_aggregator_network -e slice_genre_aggregator_host=slice_genre_aggregator -e spectrogram_filter_host=0.0.0.0 -e spectrogram_queue_host=spectrogram_queue localhost:5000/spectrogram_filter

